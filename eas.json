{
  "cli": { "version": ">= 12.0.0" },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "production": {
      "android": { "buildType": "app-bundle" },
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "https://ddapi.piaosiyuan.cn"
      }
    },
    "production-apk": {
      "android": { "buildType": "apk" },
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "https://ddapi.piaosiyuan.cn"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
